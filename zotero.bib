  author = {Sicilia, Miguel-Angel and García-Barriocanal, Elena and Sánchez-Alonso, Salvador},
  date = {2017-01-01},
  journaltitle = {Procedia Computer Science},
  shortjournal = {Procedia Computer Science},
  series = {13th {{International Conference}} on {{Current Research Information Systems}}, {{CRIS2016}}, {{Communicating}} and {{Measuring Research Responsibly}}: {{Profiling}}, {{Metrics}}, {{Impact}},{{Interoperability}}},
