  author = {Hubbard, Alan E. and Ahern, Jennifer and Fleischer, Nancy L. and family=Laan, given=Mark Van, prefix=der, useprefix=false and Lippman, Sheri A. and Jewell, Nicholas and Bruckner, Tim and Satariano, William A.},
