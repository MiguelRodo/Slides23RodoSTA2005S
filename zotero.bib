@article{zhu_etal23,
  title = {Robust Single-Cell Matching and Multimodal Analysis Using Shared and Distinct Features},
  author = {Zhu, Bokai and Chen, Shuxiao and Bai, Yunhao and Chen, Han and Liao, Guanrui and Mukherjee, Nilanjan and Vazquez, Gustavo and McIlwain, David R. and Tzankov, Alexandar and Lee, Ivan T. and Matter, Matthias S. and Goltsev, Yury and Ma, Zongming and Nolan, Garry P. and Jiang, Sizun},
  date = {2023-01-09},
  journaltitle = {Nature Methods},
  shortjournal = {Nat Methods},
