  title = {{{INFLECT}}: An {{R-package}} for Cytometry Cluster Evaluation Using Marker Modality},
  shorttitle = {{{INFLECT}}},
  author = {Verhoeff, Jan and Abeln, Sanne and Garcia-Vallejo, Juan J.},
  date = {2022-11-16},
  journaltitle = {BMC Bioinformatics},
