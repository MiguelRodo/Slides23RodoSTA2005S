@article{bothamley_etal92,
  title = {Clinical Value of the Measurement of {{Mycobacterium}} Tuberculosis Specific Antibody in Pulmonary Tuberculosis.},
  author = {Bothamley, G. H. and Rudd, R. and Festenstein, F. and Ivanyi, J.},
  date = {1992-04-01},
  journaltitle = {Thorax},
  volume = {47},
