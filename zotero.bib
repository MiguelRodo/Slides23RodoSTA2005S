  file = {C\:\\Users\\migue\\Zotero\\storage\\BCZ7Q82J\\Sicilia et al. - 2017 - Community Curation in Open Dataset Repositories I.pdf;C\:\\Users\\migue\\Zotero\\storage\\6HN28RKH\\S1877050917302776.html}
