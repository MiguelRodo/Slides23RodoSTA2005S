  title = {Male {{Sex Bias}} in {{Immune Biomarkers}} for {{Tuberculosis}}},
  author = {Bothamley, Graham H.},
  date = {2021-03-16},
  journaltitle = {Frontiers in Immunology},
  shortjournal = {Front. Immunol.},
