@article{bothamley04,
  title = {Epitope-{{Specific Antibody Levels Demonstrate Recognition}} of {{New Epitopes}} and {{Changes}} in {{Titer}} but {{Not Affinity}} during {{Treatment}} of {{Tuberculosis}}},
