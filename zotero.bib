@article{bothamley_etal88,
  title = {Humoral Response to Defined Epitopes of Tubercle Bacilli in Adult Pulmonary and Child Tuberculosis},
  author = {Bothamley, G. and Udani, P. and Rudd, R. and Festenstein, F. and Ivanyi, J.},
  date = {1988-10-01},
  journaltitle = {European Journal of Clinical Microbiology and Infectious Diseases},
